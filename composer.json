{
    "name": "boxuk/dictator",
    "description": "The Dictator controls the state of WordPress",
    "type": "wp-cli-package",
    "homepage": "https://github.com/boxuk/dictator",
    "license": "MIT",
    "authors": [
        {
            "name" : "Box UK",
            "email": "developers@boxuk.com",
            "homepage": "https://www.boxuk.com"
        },
        {
            "name": "Daniel Bachhuber",
            "email": "d@danielbachhuber.com",
            "homepage": "http://danielbachhuber.com",
            "role": "Developer"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/serializer": "^5.3",
        "symfony/yaml": "^5.3",
        "symfony/property-access": "^5.3",
        "symfony/options-resolver": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "BoxUk\\Dictator\\": "src"
        },
        "files": [ "dictator.php" ]
    },
    "require-dev": {
        "wp-cli/wp-cli-bundle": "^2.4"
    },
    "scripts": {
        "behat": "run-behat-tests",
        "behat-rerun": "rerun-behat-tests",
        "lint": "run-linter-tests",
        "phpcs": "run-phpcs-tests",
        "phpunit": "run-php-unit-tests",
        "prepare-tests": "install-package-tests",
        "test": [
            "@lint",
            "@phpcs",
            "@phpunit",
            "@behat"
        ]
    }
}
